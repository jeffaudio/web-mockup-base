{
  "name": "web-mockup-base",
  "version": "1.0.0",
  "description": "Basic web mockup with foundation.",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffaudio/web-mockup-base.git"
  },
  "keywords": [
    "mockup"
  ],
  "author": "Jeff Hentschel",
  "license": "MIT",
  "devDependencies": {
    "chalk": "^0.4.0",
    "dateformat": "^1.0.8-1.2.3",
    "gulp": "^3.8.5",
    "gulp-debug": "^0.3.0",
    "gulp-load-plugins": "^0.5.3",
    "gulp-load-utils": "0.0.4",
    "gulp-minify-css": "^0.3.6",
    "gulp-notify": "^1.4.0",
    "gulp-rimraf": "^0.1.0",
    "gulp-sass": "^0.7.2",
    "minimist": "^0.2.0"
  },
  "paths": {
    "lib": {
      "scss": [
        "./bower_components/foundation/scss"
      ]
    },
    "src": {
      "base": "./src/",
      "web": "./src/*.html",
      "php": "./src/**/*.php",
      "fonts": "./src/fonts/**/*",
      "scss": [
        "./src/sass/*.scss"
      ],
      "js": [
        "./src/js/*.js"
      ],
      "images": [
        "./src/images/**/*"
      ]
    },
    "dest": {
      "base": "./dev/",
      "css": "./dev/",
      "js": "./dev/js/",
      "images": "./dev/images/",
      "fonts": "./dev/fonts/"
    },
    "production": "./prod/"
  }
}
